 baseName = external

.{baseName} 
  &__list 
    margin-top 29px
    display grid
    grid-gap 30px 10px
    letter-spacing .6px
    grid-template-columns: 1fr 2.35fr  
  &__key
    font-size 20px
    text-transform uppercase
    color: var(--theme-blue)
    font-weight 600
  &__value
    font-size 20px